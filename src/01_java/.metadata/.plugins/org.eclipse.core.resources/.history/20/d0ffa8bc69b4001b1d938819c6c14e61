package com.lec.ex1_grade;


public class GradeMain {
	public static void main(String[] args) {
		Grade[] person = {new Grade("정우성", 90, 90, 80),
				          new Grade("김하늘", 98, 87, 88),
				          new Grade("황정민", 86, 99, 93),
				          new Grade("강동원", 85, 92, 90),
				          new Grade("유아인", 86, 98, 87)
		};
		System.out.println("                            성적표");
		System.out.println(" 이름 \t 국어 \t 영어 \t 수학 \t 합계 \t 평균");
		for(Grade p : person) {
			System.out.println(p.infoTotal());
		}
		p.infoTotal1();
	}
}
