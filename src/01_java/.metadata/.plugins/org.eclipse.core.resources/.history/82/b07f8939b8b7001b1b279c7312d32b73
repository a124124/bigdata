package com.lec.ex07_employee;

public class SalaryEmployee extends Employee {
	private int annalSalary;
	
	
	public SalaryEmployee(String name) {
		super(name);
		// TODO Auto-generated constructor stub
	}


	public SalaryEmployee(String name, int annalSalary) {
		super(name);
		this.annalSalary = annalSalary;
	}




	@Override
	public void computePay() {
		// TODO Auto-generated method stub
		annalSalary = (getComputePay() / 14);
		System.out.println("= 월급 명세서 =");
		System.out.println("성함 : " + getName());
		System.out.println("월급 : " + getComputePay());
//		System.out.println("상여금 : " + computeIncentive());
		
	}

	
}
