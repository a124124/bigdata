package com.lec.ex;
// 산술 연산자 : + - * / %
public class Ex01 {
	public static void main(String[] args) {
		int n1 = 33, n2 = 10;
		int result; double resultDouble;
		result = n1 + n2;
		System.out.printf("%d %c %d = %d\n", n1, '+', n2, result );
		result = n1 * n2;
		System.out.printf("%d %c %d = %d\n", n1, '*', n2, result );
		resultDouble = n1 / n2;
		System.out.printf("%d %c %d = %.2f\n", n1, '/', n2, result );

		
	}
}
