package com.lec.ex07_employee;

public abstract class Employee {
	private String name;
	private int computePay;

	
	
	public Employee(String name,int computePay) {
		this.computePay = computePay;
		this.name = name;
		
	}
	public abstract void computePay();
	
	public final void computeIncentive() {
		if(computePay >= 3000000 ) {
			computePay += (computePay * 1.05);
		}
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getComputePay() {
		return computePay;
	}
	public void setComputePay(int computePay) {
		this.computePay = computePay;
	}
	
}
