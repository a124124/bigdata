package com.lec.loop;

public class Ex09_quiz {
	public static void main(String[] args) {
		int su = 0;
		int su1 = 0;
		
		for (int i = 1; i <= 10; i ++) {
			if (i % 2 == 0) {
				su = su + i;
			}else {
				su1 = su1 + i;
			}
		}
		System.out.println("Â¦¼ö ÇÕ:" + su);
		System.out.println("È¦¼ö ÇÕ:" + su1);
	}
}
