package com.lec.ex04_shape;

public class TestMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Shape[] sp = {new Circle(6),
					  new Triangle(5,6),
					  new Rectangle(5,6)
		};
		for(int i = 0; i <= sp.length ; i++) {
			sp[i].draw();
			sp[i].computeArea();
		}
	}

}
