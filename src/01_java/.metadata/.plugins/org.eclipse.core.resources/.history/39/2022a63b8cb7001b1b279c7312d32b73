package com.lec.ex01_store;
//        본사 지침         : 김치찌개- 0 부대찌개- 0  비빔밥- 0 순대국- 0  공기밥- 0
public class HeadQuarterStore {
	private String name;
	
	public HeadQuarterStore(String name) {
		this.name = name;
	}
	public abstract void kimchi();  // 추상 메소드
	public void bude() {
		System.out.println("부대찌개 : 0원");
	}
	public void bibim() {
		System.out.println("비빔밥 : 0 원");
	}
	public void sunde() {
		System.out.println("순대국 : 0 원");
	}
	public void bab() {
		System.out.println("공기밥 : 0 원");
	}
	public String getName() {
		return name;
	}
	
	
	
}
