		Scanner scanner = new Scanner(system.in);
		int fn, idx = 0; // 1.대출, 2.반납, 3.도서현황, 0.종료
		String bTitle, borrower, checkOutDate; // 대출을 원하는 책이름, 대출인, 대출일
	}
}
