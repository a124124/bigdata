package com.lec.loop;

public class Ex09_quiz {
	public static void main(String[] args) {
		int su
		int su1
		
		for (i = j = 1; j <= 10; j ++) {
			if (i % 2 == 0) {
				su = su + j;
			}else {
				su1 = su1 + j;
			}
		}
		System.out.println("Â¦¼ö ÇÕ:" + su);
