package com.lec.rect;

public class RectMain {
	public static void main(String[] args) {
		Rect r1 = new Rect();
		Rect r2 = new Rect();
		
		System.out.println(r1.area());
		System.out.println(r2.area());
		// 가로 세로 세팅하는 setter를 입력.
		r1.setWidth(5);
		r1.setHeight(10);
		r2.setWidth(4);
		r2.setHeight(10);
		
		System.out.println(r1.area());
		System.out.println(r2.area());
		
		
		
	}
}
