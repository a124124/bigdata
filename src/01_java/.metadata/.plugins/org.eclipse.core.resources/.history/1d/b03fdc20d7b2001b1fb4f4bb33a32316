package com.lec.ex;
// 배열 복사
public class Ex03_arraycopy {
	public static void main(String[] args) {
		int[] score = {1, 2, 3, 4, 5};
		int[] s = new int[score.length];
		
		for (int i = 0; i < score.length; i++) {
			s[i] = score[i];
			
		}
		for(int i = 0; i < s.length; i++) {
			System.out.printf("s[%d] = %d, score[%d] = %d\n", i, s[i], i, score[i]);
		}
	}
}
