package com.lec.ex8_student;

public class StudentMain {
	public static void main(String[] args) {
		Student[] student = {new Student("정우성", 90, 90, 80),
		          new Student("김하늘", 98, 87, 88),
		          new Student("황정민", 86, 99, 93),
		          new Student("강동원", 85, 92, 90),
		          new Student("유아인", 86, 98, 87)
		};
		String[] title = {"이름", "국어", "영어", "수학", "총점", "평균"};
		for(String t : title) {
			System.out.printf("%s\t",t);
		}
		System.out.println();
		for(Student s : student) {
			s.infoToTal();
		}
	}
}
