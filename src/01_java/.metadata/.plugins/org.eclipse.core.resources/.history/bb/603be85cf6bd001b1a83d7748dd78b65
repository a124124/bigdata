package com.lec.ex02_map;

import java.sql.Date;

public class Friend {
	private String name;
	private String tel;
	private Date birth;
	
	public Friend() {
		
	}
	
	public Friend(String name, String tel) {
		super();
		this.name = name;
		this.tel = tel;
	}

	public Friend(String name, String tel, Date birth) {
		super();
		this.name = name;
		this.tel = tel;
		this.birth = birth;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return name + "의 연락처 : " + tel + ", 생일 : " + birth;
	}
	
}
